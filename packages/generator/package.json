{
  "bin": "./build/main.js",
  "dependencies": {
    "@prisma/generator-helper": "^5.8.0",
    "@prisma/internals": "^5.8.0",
    "js-yaml": "^4.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@directus/api": "^15.0.0",
    "@directus/schema": "^11.0.1",
    "@prisma/client": "^5.8.0",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.102",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-sort-exports": "^0.8.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "prettier": "^3.1.1",
    "prisma": "^5.8.0",
    "tsc-alias": "^1.8.8",
    "type-fest": "^4.9.0",
    "typescript": "^5.3.3"
  },
  "exports": {
    ".": {
      "import": "./build/index.js",
      "types": "./build/index.d.ts"
    }
  },
  "files": [
    "./build/**/*.{js,cjs,mjs,d.ts}",
    "./build/**/*.{js,cjs,mjs,d.ts}.map",
    "./src"
  ],
  "main": "./build/index.js",
  "module": "./build/index.js",
  "name": "@prisma-directus/generator",
  "private": true,
  "scripts": {
    "build": "npm run build:clean && npm run build:swc && npm run build:tsc && npm run build:tsc-alias && npm run build:prisma && npm run build:eslint",
    "build:clean": "rm -rf build",
    "build:eslint": "ESLINT_CI=1 eslint src",
    "build:prisma": "npm run dev:prisma -- generate",
    "build:swc": "swc src --out-dir build --source-maps",
    "build:tsc": "tsc --emitDeclarationOnly",
    "build:tsc-alias": "tsc-alias",
    "dev:docker:down": "docker-compose -f .docker-composed.development.yml down",
    "dev:docker:up": "docker-compose -f .docker-compose.development.yml up -d",
    "dev:prisma": "dotenv -e .env.development prisma --",
    "main": "./build/main.mjs"
  },
  "type": "module",
  "types": "./build/index.d.ts",
  "version": "0.0.0-dev"
}